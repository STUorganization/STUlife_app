{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": true,
    // "outDir": "dist",
    // "rootDir": "./src",
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*.tsx",
    "src/**/*.js",
    // "App.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
